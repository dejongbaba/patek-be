{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "date": {
      "type": "date"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "title": {
      "type": "string",
      "maxLength": 50
    },
    "description": {
      "type": "richtext"
    },
    "preview": {
      "type": "richtext",
      "maxLength": 140
    },
    "category": {
      "model": "category"
    }
  }
}
