{
  "csp": {
    "enabled": true,
    "policy": ["block-all-mixed-content"]
  },
  "p3p": {
    "enabled": true,
    "value": ""
  },
  "hsts": {
    "enabled": true,
    "maxAge": 31536000,
    "includeSubDomains": true
  },
  "xframe": {
    "enabled": true,
    "value": "SAMEORIGIN"
  },
  "xss": {
    "enabled": true,
    "mode": "block"
  },
  "cors": {
    "enabled": true,
    "origin": "*",
    "maxAge": 31536000,
    "expose": [
      "WWW-Authenticate",
      "Server-Authorization"
    ],
    "credentials": false,
    "methods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS",
      "HEAD"
    ],
    "headers": [
      "Content-Type",
      "Authorization",
      "X-Frame-Options",
      "Origin",
      "X-Requested-With",
      "Accept",
      "Client-Security-Token",
      "Accept-Encoding",
      "X-Auth-Token"
    ]
  },
  "ip": {
    "enabled": false,
    "whiteList": [],
    "blackList": []
  }
}
